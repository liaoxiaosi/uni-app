.refeeds {
  background-color: #fff;
  padding-bottom: 20rpx;
}
.one-feeds-box {
  width: 704rpx;
  margin: 4rpx 22rpx 0 22rpx;
}
.one-feeds-box .one-feed {
  position: relative;
  background-color: #dadada;
  display: grid;
  grid-template-columns: 232rpx 232rpx 232rpx;
  grid-template-rows: 232rpx  232rpx 232rpx;
  grid-row-gap: 4rpx;
  grid-column-gap: 4rpx;
  background-color: #dadada;
}
.one-feeds-box .one-feed .feedItems .feed-content {
  width: 100%;
  height: 100%;
  border: 1px solid #eee;
}
.one-feeds-box .one-feed .feedItems.feed-big-left {
  /* //网格开始的位置 */
  grid-column-start: 1;
  /* 纵向网格结束的位置 */
  grid-column-end: 3;
  /* //水平网格开始的位置 */
  grid-row-start: 1;
  grid-row-end: 3;
  background-color: darkcyan;
}
.one-feeds-box .one-feed .feedItems.feed-big-right {
  grid-column-start: 2;
  grid-column-end: 4;
  grid-row-start: 1;
  grid-row-end: 3;
  background-color: darkseagreen;
}

